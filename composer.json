{
  "name": "battis/user-session",
  "description": "User session management for Slim Framework",
  "authors": [
    {
      "name": "Seth Battis",
      "email": "seth@battis.net"
    }
  ],
  "license": "MIT",
  "require": {
    "bryanjhv/slim-session": "^4.0",
    "slim/http": "^1.2",
    "slim/php-view": "^3.2",
    "slim/psr7": "^1.5"
  },
  "autoload": {
    "psr-4": {
      "Battis\\UserSession\\": "src"
    }
  },
  "require-dev": {
    "battis/data-utilities": "^1.1",
    "battis/phpunit-sessions": "^0.1",
    "ext-simplexml": "*",
    "php-di/php-di": "^6.4",
    "phpspec/prophecy-phpunit": "^2.0"
  },
  "autoload-dev": {
    "psr-4": {
      "Battis\\UserSession\\Tests\\": "tests"
    }
  },
  "config": {
    "sort-packages": true
  }
}
